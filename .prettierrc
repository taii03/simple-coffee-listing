{
	"singleQuote": true,
	"trailingComma": "all",
	"endOfLine": "auto",
	"semi": true,
	"tabWidth": 4,
	"printWidth": 120,
	"useTabs": true,
	"plugins": ["prettier-plugin-tailwindcss", "prettier-plugin-astro", "prettier-plugin-svelte"],
	"overrides": [
		{ "files": "*.astro", "options": { "parser": "astro" } },
		{ "files": "*.svelte", "options": { "parser": "svelte" } }
	]
}
